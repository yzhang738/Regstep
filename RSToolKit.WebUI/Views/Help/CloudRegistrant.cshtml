@{
    Layout = "~/Views/Shared/_Layout_Help_Cloud.cshtml";
}
@section scripts {
    <script>
        $(function () {
            $("#popover-send-email").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Send Email',
                content: function () {
                    return $('#popover-send-email-content').html();
                }
            });
            $("#popover-link").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Confirmation / Complete',
                content: function () {
                    return $('#popover-link-content').html();
                }
            });
            $("#popover-con").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'View Confirmation',
                content: function () {
                    return $('#popover-con-content').html();
                }
            });
            $("#popover-edit").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Edit Registration',
                content: function () {
                    return $('#popover-edit-content').html();
                }
            });
            $("#popover-cancel").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Cancel Registration',
                content: function () {
                    return $('#popover-cancel-content').html();
                }
            });
            $("#popover-delete").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Cancel Registration',
                content: function () {
                    return $('#popover-delete-content').html();
                }
            });
            $("#popover-adjustment").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Record Adjustment / Payment',
                content: function () {
                    return $('#popover-adjustment-content').html();
                }
            });
            $("#popover-cc").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Process Credit Card',
                content: function () {
                    return $('#popover-cc-content').html();
                }
            });
            $("#popover-summary").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Registration Summary',
                content: function () {
                    return $('#popover-summary-content').html();
                }
            });
            $("#popover-billing").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Record Adjustment / Payment',
                content: function () {
                    return $('#popover-billing-content').html();
                }
            });
            $("#popover-transaction").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Credit Card Transaction',
                content: function () {
                    return $('#popover-transaction-content').html();
                }
            });
            $("#popover-email").popover({
                html: true,
                placement: 'top',
                trigger: 'focus',
                title: 'Email Activity',
                content: function () {
                    return $('#popover-email-content').html();
                }
            });
        });
    </script>
}
@section modals
{
    <div class="modal fade" id="send-email">
        <div class="modal-dialog">
            <div class="modal-header">
                <h3 class="modal-title">Resend Emails</h3>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-xs-12 add-padding-vertical-5">
                        <span class="glyphicon glyphicon-envelope"></span> Continue Registration
                    </div>
                    <div class="col-xs-12 add-padding-vertical-5">
                        <span class="glyphicon glyphicon-envelope"></span> Confirmation
                    </div>
                    <div class="col-xs-12 add-padding-vertical-5">
                        <span class="glyphicon glyphicon-envelope"></span> Bill Me Invoice
                    </div>
                    <div class="col-xs-12 add-padding-vertical-5">
                        <span class="glyphicon glyphicon-envelope"></span> Credit Card Receipt
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-default" data-dismiss="modal">Send Emails</button>
            </div>
        </div>
    </div><!-- /.modal-dialog -->

    <div class="modal fade" id="link">
        <div class="modal-dialog">
            <div class="modal-header">
                <h3 class="modal-title">Record Link</h3>
            </div>
            <div class="modal-body">
                <div class="row">
                    <div class="col-xs-12 add-padding-vertical">
                        Confirmation Link
                    </div>
                    <div class="col-xs-12 add-padding-vertical">
                        <code>https://toolkit.regstep.com/xxxxxxxxxxxxxxxxxxxx</code>
                    </div>
                    <div class="col-xs-12 add-padding-vertical">
                        <span class="text-color-1"><span class="glyphicon glyphicon-info-sign"></span> Copy the address above and email directly to your registrant if they cannot receive system emails.</span>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
            </div>
        </div><!-- /.modal-dialog -->
    </div><!-- /.modal -->

    <div class="modal fade" id="adjustment" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-header">
                <h3 class="modal-title">Record Adjustment / Payment</h3>
            </div>
            <div class="modal-body">
                <div class="row add-padding-vertical">
                    <div class="add-padding-bottom-10">
                        <span class="text-color-1"><span class="glyphicon glyphicon-info-sign"></span> Select Adjustment to enter an additional fee (positive amount) or a discount (negative amount). Select one of the other payment options to record a payment.</span>
                    </div>
                    <label class="control-label col-sm-3">Type</label>
                    <div class="col-sm-9">
                        <select class="form-control">
                            <option>Adjustment</option>
                            <option>Wire Payment</option>
                            <option>Credit Card Payment</option>
                            <option>Check Payment</option>
                            <option>Cash Payment</option>
                        </select>
                    </div>
                </div>
                <div class="row add-padding-vertical">
                    <div class="add-padding-bottom-10">
                        <span class="text-color-1"><span class="glyphicon glyphicon-info-sign"></span> Enter a transaction ID such as a check number, wire transfer ID, or receipt number.</span>
                    </div>
                    <label class="col-sm-3">Transaction Id</label>
                    <div class="col-sm-9">
                        <input type="text" class="form-control input-sm" />
                    </div>
                </div>
                <div class="row add-padding-vertical">
                    <div class="add-padding-bottom-10">
                        <span class="text-color-1"><span class="glyphicon glyphicon-info-sign"></span> Enter the effective date of the adjustment.</span>
                    </div>
                    <label class="col-sm-3">Date</label>
                    <div class="col-sm-9">
                        <input type="text" class="form-control input-sm" id="adjustmentDate" />
                    </div>
                </div>
                <div class="row add-padding-vertical">
                    <div class="add-padding-bottom-10">
                        <span class="text-color-1"><span class="glyphicon glyphicon-info-sign"></span> Enter the amount of adjustment. Enter a positive number. Payments are automatically deducted from the balance.</span>
                    </div>
                    <label class="control-label col-sm-3">Amount</label>
                    <div class="col-sm-9">
                        <input class="form-control input-sm" type="text" id="adjustmentAmmount" />
                    </div>
                </div>
                <div class="row add-padding-vertical">
                    <div class="add-padding-bottom-10">
                        <span class="text-color-1"><span class="glyphicon glyphicon-info-sign"></span> Enter adjustment description.</span>
                    </div>
                    <label class="control-label col-sm-3">Description</label>
                    <div class="col-sm-9">
                        <input class="form-control input-sm" type="text" id="adjustmentDescription" />
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal">Cancel</button>
                <button type="button" class="btn btn-default">Save</button>
            </div>
        </div>
    </div>

    <div class="modal fade" id="cc" data-backdrop="static" data-keyboard="false">
        <div class="modal-dialog">
            <div class="modal-header">
                <h3 class="modal-title">Process Payment</h3>
            </div>
            <div class="modal-body">
                <div class="row add-padding-bottom-10">
                    <label class="control-label col-sm-4">Transaction Type</label>
                    <div class="col-sm-8">
                        <select class="form-control input-sm" id="ccTransactionType">
                            <option>Authorize and Capture</option>
                            <option>Authorize Only</option>
                        </select>
                    </div>
                </div>
                <div class="row add-padding-bottom-10">
                    <label class="col-sm-4">Amount</label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control input-sm" />
                    </div>
                </div>
                <div class="row add-padding-bottom-10">
                    <label class="col-sm-4">Card Number</label>
                    <div class="col-sm-8">
                        <input type="text" class="form-control input-sm" />
                    </div>
                </div>
                <div class="row add-padding-bottom-10">
                    <label class="control-label col-sm-4">Name on Card</label>
                    <div class="col-sm-8">
                        <input class="form-control input-sm" type="text" />
                    </div>
                </div>
                <div class="row add-padding-bottom-10">
                    <label class="control-label col-sm-4">Zip Code</label>
                    <div class="col-sm-8">
                        <input class="form-control input-sm" type="text" />
                    </div>
                </div>
                <div class="row add-padding-bottom-10">
                    <label class="control-label col-sm-4">Card Code</label>
                    <div class="col-sm-8">
                        <input class="form-control input-sm" type="text" />
                    </div>
                </div>
                <div class="row add-padding-bottom-10">
                    <label class="control-label col-sm-4">Expiration</label>
                    <div class="col-sm-5">
                        <select class="form-control input-sm form-select">
                            <option value="01">1 (January)</option>
                            <option value="02">2 (February)</option>
                            <option value="03">3 (March)</option>
                            <option value="04">4 (April)</option>
                            <option value="05">5 (May)</option>
                            <option value="06">6 (June)</option>
                            <option value="07">7 (July)</option>
                            <option value="08">8 (August)</option>
                            <option value="09">9 (September)</option>
                            <option value="10">10 (October)</option>
                            <option value="11">11 (November)</option>
                            <option value="12">12 (December)</option>
                        </select>
                    </div>
                    <div class="col-sm-3">
                        <select class="form-control input-sm form-select">
                            @for (var i = DateTime.Now.Year; i < DateTime.Now.Year + 15; i++)
                            {
                                var twoDigit = i % 100;
                                <option value="@twoDigit">@twoDigit</option>
                            }
                        </select>
                    </div>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" data-target="#cc">Cancel</button>
                <button type="button" class="btn btn-default">Process Payment</button>
            </div>
        </div>
    </div>

}

