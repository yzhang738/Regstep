@using Microsoft.AspNet.Identity.Owin;
@{
    Layout = null;
}
<!DOCTYPE html>

<html>
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>@ViewBag.Title</title>

    <meta name="description" content="" />
    <meta name="keywords" content="" />
    <meta name="author" content="RegStep Technologies" />

    <!--CSS-->
    <link href="~/Content/Bootstrap/bootstrap.min.css" rel="stylesheet" />
    <link href="~/Content/2-regstep.min.css" rel="stylesheet" />
    <link href="~/Content/3-colors.min.css" rel="stylesheet" />
    <link href="~/Content/4-nav.min.css" rel="stylesheet" />
    <link href="~/Content/6-site.min.css" rel="stylesheet" />

    @RenderSection("styles", required: false)

    <style>
        h2 {
            color: #7A0202;
        }

        .help-breadcrumb a {
            color: #8e2828;
            text-decoration: none;
        }

            .help-breadcrumb a:hover {
                color: #7a0202;
                text-decoration: none;
            }

        .screen-shot {
            border: 1px solid #3e3e3e;
            border-radius: 4px;
        }

        .screen-shot-text {
            background: #3e3e3e;
            padding: 5px 15px;
            color: white;
        }

        .screen-shot-header {
            font-size: 20px;
        }

        .screen-shot-footer, .help-text {
            font-size: 17px;
        }

            .screen-shot-footer a, .screen-shot-footer a:hover {
                color: white;
                text-decoration: underline;
            }

        .screen-shot-body {
            padding: 15px;
        }

        a.help-nav-inactive, a.help-nav-inactive:hover {
            background: transparent !important;
            color: #ddd !important;
        }

        li.help-nav-active > a, li.help-nav-active a:hover, li.help-nav-active ul {
            background: white !important;
            color: #3e3e3e !important;
        }

        a.help-info {
            outline: none;
            border: none;
        }

        a.help-info:hover {
            cursor: pointer;
        }

        a.help-info-rsred, a.help-info-rsred:hover {
            color: #7a0202;
            text-decoration: none;
        }

        a.help-info-white, a.help-info-white:hover {
            color: white;
            text-decoration: none;
        }

        h3.popover-title {
            background: #7A0202;
            color: white;
            font-size: 15px;
        }

        .popover-content {
            color: #3e3e3e;
            font-size: 12px;
            font-weight: normal;
        }

        .popover {
            max-width: 600px;            
        }

        .popover-large {
            width: 600px;
        }

        @@media screen and (max-width: 610px) {
            .popover {
                max-width: 300px;
            }
        }

        .popover-content a {
            color: #8e2828;
            text-decoration: none;
        }

        .popover-content a:hover {
            color: #7a0202;
            text-decoration: none;
        }

        a.text-color-2:hover {
            color: #7a0202 !important;
            text-decoration: underline;
        }

        table.table-registration-data {
	        display: block;
	        overflow-x: auto !important;
        }

        table.table-registration-data th {
	        padding-right: 50px !important;
	        white-space: nowrap;
	        min-width: 150px;
        }

        th.registrant-header {
            font-size: 18px;
            color: #7a0202;
            font-weight: normal !important;
            padding-top: 15px !important;
        }
    </style>

    <!--Scripts-->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.1/jquery.min.js"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js"></script>
    <script src="~/scripts/respond.js"></script>
    <script src="~/Scripts/Tool/restful.js"></script>

    <!-- HTML5 Shim and Respond.js IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/libs/html5shiv/3.7.0/html5shiv.js"></script>
    <![endif]-->
</head>
<body>
    <div>
        <!--Wrapper-->
        <div class="container-fluid">

            <!--Header-->
            <header>
                <div class="container-fluid color-white">
                    <!--Logo Bar-->
                    <div class="row add-padding-vertical-5">
                        <div class="col-xs-12">
                            <img class="img-displayed img-max-width-fixed-200 img-left" src="/images/common/regstep.png" alt="RegStep" />
                        </div>
                    </div>
                    <!--End Logo Bar-->
                    <!--Navigation-->
                    <div class="row add-padding-top-5 color-grey-4">
                        <div class="col-xs-12">
                            <nav class="navbar navbar-utility" role="navigation">
                                <div class="container-fluid">
                                    <!--Mobile Items-->
                                    <div class="navbar-header">
                                        <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#nav2">
                                            <span class="sr-only">Toggle navigation</span>
                                            <span class="icon-bar"></span>
                                            <span class="icon-bar"></span>
                                            <span class="icon-bar"></span>
                                        </button>
                                    </div>
                                    <!--End Mobile Items-->
                                    <!--Nav Items-->
                                    <div class="collapse navbar-collapse" id="nav2">
                                        <ul class="nav navbar-nav navbar-help">
                                            <!--Overview-->
                                            <li class="help-toc">
                                                <a href="~/Help/TOC">
                                                    Table of Contents
                                                </a>
                                            </li>
                                            <!--End Overview-->
                                            <!--Overview-->
                                            <li class="help-home">
                                                <a href="~/Help/Index">
                                                    Home
                                                </a>
                                            </li>
                                            <!--End Overview-->
                                            <!--Cloud-->
                                            <li class="help-cloud">
                                                <a href="~/Help/Cloud">
                                                    Cloud
                                                </a>
                                            </li>
                                            <!--End Cloud-->
                                            <!--Forms-->
                                            <li class="help-forms">
                                                <a href="~/Help/FormBuilder">
                                                    Forms
                                                </a>
                                            </li>
                                           <!--End Forms-->
                                            <!--Surveys
                                            <li class="help-surveys">
                                                <a href="~/Help/Surveys">
                                                    Surveys
                                                </a>
                                            </li>
                                            End Surveys-->
                                            <!--Email
                                            <li class="help-email">
                                                <a href="~/Help/Email">
                                                    Email
                                                </a>
                                            </li>
                                            End Email-->
                                            <!--Admin
                                            <li class="help-admin">
                                                <a href="~/Help/Admin">
                                                    Admin
                                                </a>
                                            </li>
                                            End Admin-->
                                        </ul>
                                    </div>
                                    <!--End Nav Items-->
                                </div>
                            </nav>
                        </div>
                    </div>
                    <!--End Navigation-->
                </div>
            </header>
            <!--End Header-->
            <!--Work Area-->
            <div class="container-fluid container-content color-white add-padding-vertical">
                <div class="row">
                    <div class="col-xs-12">
                        <div class="row">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
            <!--End Work Area-->
            <!--Footer-->
            <footer>
                <div class="container-fluid text-align-center add-padding-vertical-5 text-small text-color-white">
                    &copy; 2013-@DateTime.UtcNow.Year RegStep Technologies
                </div>
            </footer>
            <!--End Footer-->
        </div>
        <!--End Wrapper-->
    </div>

    <!--Modals-->
    @RenderSection("modals", required: false)
    <!--End Modals-->

    <!-- JavaScript Section-->
    @RenderSection("scripts", required: false)
    <!-- End Javascript Section -->
</body>
</html>
